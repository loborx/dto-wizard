{
    "name": "loborx/dto-wizard",
    "description": "Tranform arrays with data into objects",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "fakerphp/faker": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "symfony/var-dumper": "^6.2",
        "pestphp/pest": "^2.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Loborx\\DtoWizard\\": "src/",
            "Loborx\\DtoWizardExamples\\": "examples/",
            "Loborx\\DtoWizardTests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Piotr Skrobol",
            "email": "pio.skro@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
